# Uncomment the next line to define a global platform for your project
 platform :ios, '13.0'

use_frameworks!

def sharepods
  # Pods for NOVA
  pod 'ImagePicker', '~> 3.2'
  pod 'Firebase/Analytics'
  pod 'Firebase/Auth'
  pod 'Firebase/Storage'
  pod 'Firebase/Firestore'
  pod 'FirebaseFirestoreSwift'
  pod 'AnyFormatKit'
end

target 'NOVA' do
  sharepods
end


target 'NOVA Dev' do
  sharepods
end
